        <activity android:name="zx9.staris.numbaseball.ingame"></activity>
        <activity android:name="zx9.staris.numbaseball.MainActivity"></activity>
        <activity android:name="zx9.staris.numbaseball.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
